{
  "routes": [
    {
      "route": "/images/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=15770000"
      }
    },
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=15770000"
      }
    },
    {
      "route": "/api/assessment/create",
      "methods": ["POST"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/assessment/save",
      "methods": ["POST"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"],
      "rewrite": "/index.html",
      "headers": {
        "cache-control": "no-cache"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*",
      "/static/css/*",
      "/static/js/*",
      "/static/media/*",
      "/api/*",
      "/manifest.json",
      "/robots.txt",
      "/favicon.ico"
    ]
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'; style-src 'self' https: 'unsafe-inline'; script-src 'self' https: 'unsafe-eval' 'unsafe-inline'; img-src 'self' https: data: blob:;",
    "X-Content-Type-Options": "nosniff"
  },
  "mimeTypes": {
    ".html": "text/html",
    ".css": "text/css",
    ".js": "application/javascript",
    ".json": "application/json",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".gif": "image/gif",
    ".svg": "image/svg+xml",
    ".ico": "image/x-icon",
    ".ttf": "font/ttf",
    ".woff": "font/woff",
    ".woff2": "font/woff2"
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
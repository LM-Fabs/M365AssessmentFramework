{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "postgresqlAdminUsername": {
      "value": "assessment_admin"
    },
    "postgresqlAdminPassword": {
      "value": "${POSTGRES_ADMIN_PASSWORD}"
    },
    "databaseName": {
      "value": "m365_assessment"
    },
    "postgresqlVersion": {
      "value": "16"
    },
    "postgresqlSkuName": {
      "value": "Standard_B1ms"
    },
    "postgresqlStorageSize": {
      "value": 32
    },
    "backupRetentionDays": {
      "value": 7
    },
    "geoRedundantBackup": {
      "value": "Disabled"
    },
    "highAvailabilityMode": {
      "value": "Disabled"
    },
    "publicNetworkAccess": {
      "value": "Disabled"
    },
    "enablePrivateEndpoints": {
      "value": true
    },
    "staticWebAppName": {
      "value": "swa-${AZURE_ENV_NAME}"
    },
    "staticWebAppSku": {
      "value": "Standard"
    },
    "appInsightsName": {
      "value": "appi-${AZURE_ENV_NAME}"
    },
    "logAnalyticsName": {
      "value": "law-${AZURE_ENV_NAME}"
    },
    "keyVaultName": {
      "value": "kv-${AZURE_ENV_NAME}"
    }
  }
}
